{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Android App",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net7.0-android/DayOnes.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "logging": {
        "moduleLoad": false
      },
      "internalConsoleOptions": "openOnSessionStart",
      "launchBrowser": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
